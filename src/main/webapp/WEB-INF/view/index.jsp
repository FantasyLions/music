<script type="text/javascript">
function followMeOver() {
  $("#layoutBg,#followMeLayout").hide();
  $("#followMe").children().hide();
  $("html,body").animate({scrollTop: 0}, 500);
};
function startGuide() {
 $("#layoutBg,#followMeLayout,#followMe,#follow01bg,#follow01,#followStep1,#followStep1mask").show();
 $("#layoutBg").height(document.body.scrollHeight);
 $("html,body").animate({scrollTop: 0}, 500);
 $("#follow01continue,#follow02continue,#follow03continue,#follow04continue").click(function() {
    var arr = ['follow01continue', 'follow02continue', 'follow03continue', 'follow04continue'];
    var index = arr.indexOf($(this)[0].id);
    if (index != -1) {
    index += 1;
    var format = common.util.stringFormat;
    var h1 = format('#follow0{0}bg,#follow0{0},#followStep{0},#followStep{0}mask', index);
    var h2 = format('#follow0{0}bg,#follow0{0},#followStep{0}', index + 1);
    $(h1).hide();
    $(h2).show();
    }
    if (index == 3) {
    $("html,body").animate({scrollTop: $("#follow04bg").offset().top - 200}, 500);
    }
  });
};
$.fn.tabs_01 = function(options) {
    return this.each(function() {
       var settings = $.extend({count: 0,type: 1}, options || {});
       var current = this;
       settings.sWidth = $(current).siblings(".tabs_01_swap").width();
       if (settings.type == 1) {
       settings.len = $(current).siblings(".tabs_01_swap").find("li").length;
       } else {
       settings.len = $(current).siblings(".tabs_01_swap").find("div.m_tabs_item").length;
       settings.count = settings.count / 9;
       }
       settings.page = Math.ceil(settings.len / settings.count);
       settings.index = 0;

       $(current).find("em").eq(1).html(settings.page); //设置页数
       //上一页按钮
       $(current).find(".left").click(function() {
       settings.index -= 1;
        if (settings.index == -1) {
           settings.index = settings.page - 1;
        }
           showPics(settings.index);
           return false;
        });
       //下一页按钮
       $(current).find(".right").click(function() {
          settings.index += 1;
          if (settings.index == settings.page) {
             settings.index = 0;
           }
           showPics(settings.index);
           return false;
        });
        //本例为左右滚动，即所有li元素都是在同一排向左浮动，所以这里需要计算出外围ul元素的宽度
        $(current).siblings(".tabs_01_swap").find(".tabs_01_con").css("width", settings.sWidth * (settings.page));
        //显示图片函数，根据接收的index值显示相应的内容

        function showPics(index) { //普通切换
            $(current).find("em").eq(0).html(index + 1);
            var nowLeft = -index * settings.sWidth; //根据index值计算ul元素的left值
            $(current).siblings(".tabs_01_swap").find(".tabs_01_con").stop(true, false).animate({"left": nowLeft}, 300, function() {
                lazy.resize();
            }); //通过animate()调整ul元素滚动到计算出的position
        }
    })
};
//图片延迟加载代码
window.lazy = new ImagesLazyLoad({
    mode: "dynamic",
    attribute: "original",
    delay: 300,
    holder: "${path}/Images/o_dot.gif"
});

(function($) {
     $('.xg_play').bind('click', function(event) {
      event.stopPropagation();
      event.preventDefault();
      var itemid = $(this).attr('itemid');
      var type = $(this).attr('type');
      if (itemid != "") {
        window.top.Player.toPlayList(itemid, type, false, $(this));
      }
    });
    $("#m_tabs_01").tabs_01({count: 6});
    $("#m_tabs_02").tabs_01({count: 5});
    $("#m_tabs_03").tabs_01({count: 9,type: 2});

    lazy.resize();
    //音乐标签
    $(".g_lybq .u_tags li").each(function() {
        $(this).addClass(getRandomNum(1, 7));
    })

    function getRandomNum(min, max) {
        var range = max - min;
        var rand = Math.random();
        var random = min + Math.round(rand * range);
        var arr = ["u_tags_red", "u_tags_red2", "u_tags_yellow", "u_tags_blue", "u_tags_green", "u_tags_gray"];
        if (random - 1 >= 0) return arr[random];
        else return null;
    };
    //tab切换
    var $cssTabs = $(".m_tabs li");
    $(".css-panes .css-panes-con:first-child").show();
    $cssTabs.mouseover(function() {
        $(this).addClass("current").siblings().removeClass("current");
        $(this).parent().next().find(".m_tabs_item").hide().eq($(this).index()).show();
        if ($(this).index() == 0) {
            $(this).parent().siblings(".sprt_index").removeClass("sprt_index_title_yyphb").addClass("sprt_index_title_yyxgb");
        } else {
            $(this).parent().siblings(".sprt_index").removeClass("sprt_index_title_yyxgb").addClass("sprt_index_title_yyphb");
        }
    });
    //导航tabs
    $(".g_rank .m_tabs li").mouseover(function() {
        if ($(this).index() == 1) {
            $(this).parent().addClass("cur")
        } else {
            $(this).parent().removeClass("cur")
        }
    })
    //m_banner图片滚动
    var $lof = $(".m_banner_wapper li");
    var lof_len = $lof.length - 1;
    var lof_flag = 0;
    var lof_auto = setInterval(showNext, 3000);
    var pre_next = $(".m_banner_pre,.m_banner_next");
    $(".m_banner_con").hover(function() {
        pre_next.show();
        clearInterval(lof_auto);
    }, function() {
        pre_next.hide();
        lof_auto = setInterval(showNext, 3000);
    });
    lof_init(0); //初始化
    $(".m_banner_navigator li").hover(function() {
        lof_flag = $(".m_banner_navigator li").index($(this)[0])
        lof_init(lof_flag);
    });
    $(".m_banner_pre").click(function() {
        showPre();
        return false;
    });
    $(".m_banner_next").click(function() {
        showNext();
        return false;
    });

    function lof_init(index) {
        $lof.hide().eq(index).show().parent().next().find("li").removeClass("active").eq(index).addClass("active");
    }

    function showNext() {
        if (!$lof.is(":animated")) {
            if (lof_flag == lof_len) {
                lof_init(0);
                lof_flag = 0;
            } else {
                lof_flag++;
                lof_init(lof_flag);
            }
        }
    };

    function showPre() {
        if (!$lof.is(":animated")) {
            $lof.hide().removeClass("active");
            if (lof_flag == 0) {
                lof_init(lof_len);
                lof_flag = lof_len;
            } else {
                lof_flag--;
                lof_init(lof_flag);
            }
        }
    };

    //会员日JS
    var date = new Date();var week = date.getDay();var hours = date.getHours();
    if (week == 3 || (week == 2 && hours >= 19)) {
        $("#huiyuanri").show();
    }
})(jQuery);
</script>
